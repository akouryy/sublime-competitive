<snippet>
    <content><![CDATA[/* <sr.m.gcd> */
    // VERIFIED: arc004c
    // O(log(A+B))
    int gcd(int a, int b) { return b == 0 ? a : gcd(b, a%b); }
    // NOT_VERIFIED
    // O(log(A+B))
    inline int lcm(int a, int b) { return a / gcd(a, b) * b; }
/* </sr.m.gcd> */]]></content>
    <tabTrigger>m-gcd-lcm</tabTrigger>
